<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="codename.controleur.AccueilControleur"
           stylesheets="@/public/styles.css"
           styleClass="anchor-pane"
           prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button mnemonicParsing="false"
                text="JOUER"
                onAction="#handleJouer"
                styleClass="button">
            <font>
                <Font name="Comic Sans MS Bold" size="21.0" />
            </font>
        </Button>
    </children>
</StackPane>
